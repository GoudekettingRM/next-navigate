{
  "name": "next-navigate",
  "version": "0.2.2",
  "description": "Help navigate your NextJS application without hard-coded routing",
  "main": "dist/src/index.js",
  "module": "dist/src/index.mjs",
  "types": "dist/src/next-navigate.d.ts",
  "scripts": {
    "dev": "node bin/developAutoBuild.js",
    "build": "tsup --entry bin/setup/init.ts --entry src/index.ts  --entry bin/watch.ts --entry src/useNavigation.ts --format cjs,esm --dts && npm run copy-hooks",
    "copy-hooks": "cp src/useNavigation.ts dist/src/useNavigation.ts",
    "release": "npm run build && changeset publish",
    "lint": "tsc"
  },
  "bin": {
    "next-navigate": "./bin/init.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoudekettingRM/next-navigate.git"
  },
  "keywords": [
    "nextjs",
    "next",
    "navigation",
    "routing"
  ],
  "author": "Robin Goudeketting",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/GoudekettingRM/next-navigate/issues"
  },
  "homepage": "https://github.com/GoudekettingRM/next-navigate#readme",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/react": "^18.2.22",
    "chokidar": "^3.5.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "next": "^13.5.3",
    "rc": "^1.2.8",
    "react": "^18.2.0"
  }
}
